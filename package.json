{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "proyeto guia de hoteles",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin price.html -d dist --htmlmin -o dist/price.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin conditions.html -d dist --htmlmin -o dist/conditions.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://LaylaRodas@bitbucket.org/laylarodas/guia-hoteles.git"
  },
  "author": "Layla Rodas",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/laylarodas/guia-hoteles/issues"
  },
  "homepage": "https://bitbucket.org/laylarodas/guia-hoteles#readme",
  "devDependencies": {
    "browser-sync": "^2.27.11",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^8.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^4.1.2",
    "sass": "^1.57.1",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@primer/octicons": "^17.11.1",
    "bootstrap": "^4.6.2",
    "concurrently": "^7.6.0",
    "jquery": "^3.6.3",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "uglifyjs": "^2.4.11"
  }
}
